<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Kaos.Physics</RootNamespace>
    <AssemblyName>KaosPhysics</AssemblyName>
    <Version>0.1.0</Version>
    <Authors>github.com/kaosborn</Authors>
    <Company />
    <Description>Immutable table of the elements.</Description>
    <RepositoryUrl>https://github.com/kaosborn/KaosPhysics</RepositoryUrl>
    <Copyright>Copyright Â© 2021 github.com/kaosborn</Copyright>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>chemistry elements nuclides isotopes atomic periodic table</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/kaosborn/KaosPhysics</PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>obj\Release\netcoreapp3.1\KaosPhysics.xml</DocumentationFile>
  </PropertyGroup>

  <Import Project="..\Source\KaosPhysics\KaosPhysics.projitems" Label="Shared" />

</Project>
